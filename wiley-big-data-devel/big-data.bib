@article{giedd_brain_1999,
  title = {Brain development during childhood and adolescence: a longitudinal {MRI} study},
  volume = {2},
  copyright = {© 1999 Nature Publishing Group},
  issn = {1097-6256},
  shorttitle = {Brain development during childhood and adolescence},
