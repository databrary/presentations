@article{
   author = {Borgman, Christine L.},
   title = {The conundrum of sharing research data},
   journal = {Journal of the American Society for Information Science & Technology},
   volume = {63},
   number = {6},
   pages = {1059-1078},
   note = {Borgman, Christine L. 1; Affiliations: 1: asi22634-aff-0001; Issue Info: Jun2012, Vol. 63 Issue 6, p1059; Thesaurus Term: RESEARCH; Thesaurus Term: INFORMATION resources management; Thesaurus Term: ACCESS to information; Subject Term: LAW & legislation; Subject Term: GREAT Britain -- Politics & government; Subject Term: UNITED States -- Politics & government; Subject Term: EXPERIMENTAL design; Subject Term: MEDICAL research; Subject Term: SCIENCE; Subject Term: SERIAL publications; Subject Term: INFORMATION overload; Subject: GREAT Britain; Subject: UNITED States; Author-Supplied Keyword: collaboration; Author-Supplied Keyword: information policy; Author-Supplied Keyword: information reuse; Author-Supplied Keyword: motivation; Author-Supplied Keyword: research data sets; Number of Pages: 19p; Document Type: Article},
   abstract = {Researchers are producing an unprecedented deluge of data by using new methods and instrumentation. Others may wish to mine these data for new discoveries and innovations. However, research data are not readily available as sharing is common in only a few fields such as astronomy and genomics. Data sharing practices in other fields vary widely. Moreover, research data take many forms, are handled in many ways, using many approaches, and often are difficult to interpret once removed from their initial context. Data sharing is thus a conundrum. Four rationales for sharing data are examined, drawing examples from the sciences, social sciences, and humanities: (1) to reproduce or to verify research, (2) to make results of publicly funded research available to the public, (3) to enable others to ask new questions of extant data, and (4) to advance the state of research and innovation. These rationales differ by the arguments for sharing, by beneficiaries, and by the motivations and incentives of the many stakeholders involved. The challenges are to understand which data might be shared, by whom, with whom, under what conditions, why, and to what effects. Answers will inform data policy and practice. [ABSTRACT FROM AUTHOR]
