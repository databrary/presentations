@article{
   author = {Ardini, Mary-Anne and Pan, Huaqin and Qin, Ying and Cooley, Philip C.},
   title = {Sample and data sharing: Observations from a central data repository},
   journal = {Clinical Biochemistry},
   volume = {47},
